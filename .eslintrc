{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "ignorePatterns": ["**/node_modules/**/*.*"],
  "plugins": ["prettier"],
  "env": {
    "node": true
  },
  "rules": {
    "no-console": 0,
    "no-use-before-define": 0,
    "prettier/prettier": [
      "error"
    ],
    "import/prefer-default-export": 0
  }
}